{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/addanother": "^1.2",
        "drupal/admin_toolbar": "^3.0",
        "drupal/advagg": "^6.0@alpha",
        "drupal/advancedqueue": "^1.0@beta",
        "drupal/backup_migrate": "^5.0",
        "drupal/blazy": "^2.22",
        "drupal/bootstrap": "^3.30",
        "drupal/captcha": "^2.0",
        "drupal/commerce": "^2.36",
        "drupal/commerce_paypal": "^1.4",
        "drupal/commerce_webform_order": "^3.0@beta",
        "drupal/consumer_image_styles": "^4.0",
        "drupal/core-composer-scaffold": "^10.0",
        "drupal/core-project-message": "^10.0",
        "drupal/core-recommended": "^10.0",
        "drupal/devel": "^5.1",
        "drupal/devel_entity_updates": "^4.1",
        "drupal/ds": "^3.15",
        "drupal/eck": "^2.0",
        "drupal/encrypt": "^3.1",
        "drupal/entity_reference_revisions": "^1.10",
        "drupal/facebook_pixel": "^2.0@RC",
        "drupal/faq": "^1.2",
        "drupal/filefield_paths": "^1.0@beta",
        "drupal/google_analytics": "^4.0",
        "drupal/graphql": "^4.0",
        "drupal/health_check": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/key": "^1.17",
        "drupal/mailsystem": "^4.4",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/metatag": "^1.23",
        "drupal/migrate_tools": "^6.0",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/phpmailer_smtp": "^2.2",
        "drupal/recaptcha": "^3.2",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/redirect": "^1.8",
        "drupal/schema_metatag": "^2.5",
        "drupal/shield": "^1.7",
        "drupal/slick": "^2.10",
        "drupal/slick_extras": "^1.0@RC",
        "drupal/slick_views": "^2.8",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/stripe": "^2.0@beta",
        "drupal/stripe_webform": "^2.0@beta",
        "drupal/tfa": "^2.0@alpha",
        "drupal/typed_data": "^1.0@beta",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/webform": "^6.2@beta",
        "drupal/webform_encrypt": "^2.0@alpha",
        "drupal/webform_views": "^5.2",
        "drupal/xmlsitemap": "^1.4",
        "drush/drush": "*",
        "enshrined/svg-sanitize": "^0.16.0",
        "stripe/stripe-php": "^7.128",
        "webonyx/graphql-php": "^14.0",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": false,
            "wikimedia/composer-merge-plugin": false
        },
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
	"patches": {
        	"drupal/slick": {
		        "Issues #3343829: slick-fix-deprecation-warning": "https://www.drupal.org/files/issues/2023-02-23/3343829-2.patch"	
		}
    	}
    }	
}
